{
    "callHierarchy.splitCommand": "bel 10sp",
    "yaml.enable": true,
    "pylsp.enable": false,
    "pylsp.plugins.yapf.enabled": true,
    "python.formatting.provider": "yapf",
    "python.linting.flake8Enabled": true,
    "python.linting.ruffEnabled": false,
    "python.linting.pylintArgs": ["--load-plugins", "pylint_flask_sqlalchemy", "pylint_flask"],
    "clojure.initialization-options.dependency-scheme": "jar",
    "clojure.initialization-options.ignore-classpath-directories": true,
    "clojure.initialization-options.semantic-tokens?": true,
    "clojure.initialization-options.text-document-sync-kind": ":full",
    "clojure.initialization-options.use-metadata-for-privacy?": true,
    "codeLens.enable": true,
    "codeLens.position": "eol",
    "codeLens.separator": "  ",
    "semanticTokens.filetypes": ["*"],
    /* "clojure.startup-message": true,
       "clojure.trace.server": "verbose",
       "clojure.lsp-version": "latest",
       "clojure.enable": false, */
    "signature.enable": true,
    "notification.border": false,
    "diagnostic.floatConfig": {"highlight": "Pmenu"},
    "suggest.minTriggerInputLength": 2,
    "suggest.maxCompleteItemCount": 15,
    "suggest.removeDuplicateItems": true,
    "suggest.completionItemKindLabels": {
      "class": " פּ ",
      "color": "  ",
      "constant": "  ",
      "constructor": "  ",
      "enum": " 練",
      "enumMember": "  ",
      "event": "  ",
      "field": "  ",
      "file": "  ",
      "folder": " ﱮ ",
      "function": " λ  ",
      "interface": " 蘒",
      "keyword": " : ",
      "method": "  ",
      "module": "  ",
      "operator": "  ",
      "property": "  ",
      "reference": "  ",
      "snippet": "  ",
      "struct": "  ",
      "text": "  ",
      "typeParameter": "<>",
      "unit": " 塞 ",
      "value": "  ",
      "variable": "[]"
    },
    "tree.key.activeFilter": "f",
    "languageserver": {
      "clojure-lsp": {
        "command": "/home/linuxbrew/.linuxbrew/bin/clojure-lsp",
        "filetypes": ["clojure"],
        "rootPatterns": ["project.clj", "deps.edn"],
        "additionalSchemes": ["jar", "zipfile"],
        "trace.server": "verbose",
        "diagnostic.showUnused": true,
        "diagnostic.showDeprecated": true,
        "diagnostic.highlightPriority": 1000000,
        "initializationOptions": {
          "ignore-classpath-directories": true
        },
        "progressOnInitialization": true
      }
    }
}
